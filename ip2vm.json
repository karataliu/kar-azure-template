{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "input": {
      "type": "object",
      "metadata": {
        "description": "ipConfiguration info."
      }
    }
  },
  "variables": {
    "vmidPrefix": "[concat('/subscriptions/',subscription().subscriptionId,'/resourceGroups/',resourceGroup().name,'/providers/Microsoft.Compute/virtualMachines/')]"
  },
  "resources": [],
  "outputs": {
    "output": { 
        "value": { 
          "vmName":"[replace(reference(concat(parameters('input').id, '/../../'), '2016-03-30').virtualMachine.id, variables('vmidPrefix'),'')]"},
        "type" : "object"
    }
  }
}
